<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory app:title="@string/title_pref_incoming_call">
        <SwitchPreferenceCompat
            app:key="inCallInternational"
            app:title="@string/title_pref_incoming_call_international" />
        <SwitchPreferenceCompat
            app:key="inCallDomestic"
            app:title="@string/title_pref_incoming_call_domestic" />
    </PreferenceCategory>
    <PreferenceCategory app:title="@string/title_pref_general">
        <SwitchPreferenceCompat
            app:key="autoStart"
            app:title="@string/title_pref_general_autostart" />
    </PreferenceCategory>
</PreferenceScreen>